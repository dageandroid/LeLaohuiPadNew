<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
<include
    layout="@layout/llh_head_view"
    ></include>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimens/ser_order_hight">
        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/start_time"
            style="@style/filterSubscribeTimeStyle"
            android:text="开始时间"
            android:visibility="gone"
            />
        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/stop_time"
            style="@style/filterSubscribeTimeStyle"
            android:text="结束时间"
            />
        <android.support.v7.widget.AppCompatButton
            android:id="@+id/querySubscribe"
            style="@style/filterSubscribeButtonStyle"
            android:text="预约"
            />
    </LinearLayout>
    <View
        style="@style/ViewStyle">
    </View>
    <android.support.v7.widget.RecyclerView
        android:id="@+id/query_ser_subscribe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </android.support.v7.widget.RecyclerView>
</LinearLayout>
